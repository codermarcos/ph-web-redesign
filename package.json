    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "aws-cdk": "^2.38.1",
    "aws-cdk-lib": "^2.38.1",
    "concurrently": "^7.6.0",
    "constructs": "^10.1.84",
    "cross-env": "^7.0.3",
