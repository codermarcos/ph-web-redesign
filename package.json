{
  "name": "ph-web-redesign",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "cdk:synth": "cdk synth",
    "cdk:bootstrap": "cdk bootstrap --toolkit-stack-name ProductHuntCodermarcos --qualifier aca28edf9c",
    "cdk:deploy": "cross-env NODE_ENV=production cdk deploy",
    "cypress": "cypress open",
    "dev": "concurrently \"npm:next:dev\" \"npm:ssl\"",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless",
    "lint": "next lint",
    "next:dev": "next dev -p 3001",
    "ssl": "local-ssl-proxy --source 3000 --target 3001",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci"
  },
  "dependencies": {
    "@apollo/client": "^3.7.5",
