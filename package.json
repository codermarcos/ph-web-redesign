    "cookie": "^0.5.0",
    "csx": "^10.0.2",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.0",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.4.1",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@aws-cdk/aws-lambda": "^1.169.0",
    "@aws-cdk/core": "^1.169.0",
    "@faker-js/faker": "^7.6.0",
