    "@swc/register": "^0.1.10",
    "@testing-library/jest-dom": "5.16.4",
    "@types/cookie": "^0.5.1",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
